<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierMgmtDBDAOSearchTrdCdRSQL">
			<desc><![CDATA[Search TrdCd]]></desc>
			<sql><![CDATA[
SELECT TRD_CD
FROM MDM_TRADE
WHERE 1 = 1
AND DELT_FLG = 'N'
AND TRD_CD = @[trd_cd]			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
